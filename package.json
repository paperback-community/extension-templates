{
  "name": "extension-templates",
  "description": "Extension templates repository for Paperback.",
  "version": "0.9.0",
  "author": "Paperback Community",
  "license": "MIT",
  "private": true,
  "keywords": [
    "iOS",
    "Paperback",
    "extension",
    "templates"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paperback-community/extension-templates.git"
  },
  "bugs": {
    "url": "https://github.com/paperback-community/extension-templates/issues"
  },
  "homepage": "https://github.com/paperback-community/extension-templates#readme",
  "type": "module",
  "scripts": {
    "tsc": "tsc --project tsconfig.json --noEmit",
    "eslint-check": "eslint --config .eslintrc.js '**/*{.js,.ts}'",
    "eslint-write": "eslint --config .eslintrc.js --fix '**/*{.js,.ts}'",
    "prettier-check": "prettier --config .prettierrc.js --check '**/*{.md,.yaml,.json,.js,.ts}'",
    "prettier-write": "prettier --config .prettierrc.js --write '**/*{.md,.yaml,.json,.js,.ts}'",
    "bundle": "paperback-cli bundle",
    "serve": "paperback-cli serve",
    "logcat": "paperback-cli logcat",
    "nodemon": "nodemon -w \"./src/**/*.ts\" -e \"ts\" -x \"npm run serve\"",
    "prepare": "husky",
    "test": "run-s tsc eslint-check prettier-check bundle"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.0",
    "@eslint/js": "^9.12.0",
    "@paperback/toolchain": "^1.0.0-alpha.21",
    "@paperback/types": "^1.0.0-alpha.21",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.7.6",
    "eslint": "^9.12.0",
    "husky": "^9.1.6",
    "nodemon": "^3.1.7",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.10.0"
  }
}
